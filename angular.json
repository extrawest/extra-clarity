{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "datagrid-filters": {
      "projectType": "library",
      "root": "projects/datagrid-filters",
      "sourceRoot": "projects/datagrid-filters/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/datagrid-filters/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/datagrid-filters/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/datagrid-filters/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/datagrid-filters/src/test.ts",
            "tsConfig": "projects/datagrid-filters/tsconfig.spec.json",
            "karmaConfig": "projects/datagrid-filters/karma.conf.js"
          }
        }
      }
    },
    "extra-clarity-lib": {
      "projectType": "library",
      "root": "projects/extra-clarity-lib",
      "sourceRoot": "projects/extra-clarity-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/extra-clarity-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/extra-clarity-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/extra-clarity-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/extra-clarity-lib/src/test.ts",
            "tsConfig": "projects/extra-clarity-lib/tsconfig.spec.json",
            "karmaConfig": "projects/extra-clarity-lib/karma.conf.js"
          }
        }
      }
    }
  }
}
