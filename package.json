{
  "name": "extra-clarity",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:extra-clarity": "ng build extra-clarity",
    "watch": "ng build --watch --configuration development",
